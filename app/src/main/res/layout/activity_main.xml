<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00460707"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="729dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp">

        <com.example.customcomps.MyCustomButton
            android:id="@+id/myCustomButton12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="com.example.customcomps.MyCustomButton"
            app:buttonIcon="arrowdown" />

        <com.example.customcomps.MyCustomButton
            android:id="@+id/myCustomButton13"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="com.example.customcomps.MyCustomButton"
            app:buttonIcon="date" />

        <com.example.customcomps.MyCustomButton
            android:id="@+id/myCustomButton14"
            android:layout_width="match_parent"
            android:layout_height="51dp"
            android:text="com.example.customcomps.MyCustomButton"
            app:buttonIcon="filter" />

        <com.example.customcomps.MyCustomButton
            android:id="@+id/myCustomButton16"
            android:layout_width="match_parent"
            android:layout_height="52dp"
            android:text="com.example.customcomps.MyCustomButton"
            app:buttonIcon="save" />

        <com.example.customcomps.MyCustomButton
            android:id="@+id/myCustomButton17"
            android:layout_width="match_parent"
            android:layout_height="53dp"
            android:text="com.example.customcomps.MyCustomButton"
            app:buttonIcon="list" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>