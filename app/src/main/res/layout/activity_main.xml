<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00460707"
    tools:context=".MainActivity">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:layout_marginBottom="1dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.example.customcomps.MyCustomButton
            android:id="@+id/myCustomButton12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="com.example.customcomps.MyCustomButton"
            app:buttonIcon="arrowdown" />

        <com.example.customcomps.MyCustomButton
            android:id="@+id/myCustomButton13"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="com.example.customcomps.MyCustomButton"
            app:buttonIcon="date" />

        <com.example.customcomps.MyCustomButton
            android:id="@+id/myCustomButton14"
            android:layout_width="match_parent"
            android:layout_height="51dp"
            android:text="com.example.customcomps.MyCustomButton"
            app:buttonIcon="filter" />

        <com.example.customcomps.MyCustomButton
            android:id="@+id/myCustomButton17"
            android:layout_width="match_parent"
            android:layout_height="53dp"
            android:text="com.example.customcomps.MyCustomButton"
            app:buttonIcon="list" />

        <com.example.customcomps.UyumSpinner
            android:id="@+id/uyumSpinner"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            app:FieldToShow="field1"
            app:FieldToReturn="field2"
            app:Label="deneme"
            app:MethodName="ListObject"
            app:WebServiceUrl="http://192.168.1.232/WebService1.asmx" />

        <Spinner
            android:id="@+id/spinner2"
            android:layout_width="match_parent"
            android:layout_height="58dp" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>